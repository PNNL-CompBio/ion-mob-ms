Bootstrap: docker
From: minio/minio
Includecmd: yes


%environment
    NODE_ENV=development
    PORT=9000
    PORT=9090
    ALLOW_HTTP=true
    URL=localhost
    MINIO_ROOT_USER=minio
    MINIO_ROOT_PASSWORD=minio123
    export NODE_ENV PORT ALLOW_HTTP URL MINIO_ROOT_USER MINIO_ROOT_PASSWORD

%startscript
    minio server --console-address ":9090" /data